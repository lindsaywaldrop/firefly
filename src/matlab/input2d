%
% FLUID PARAMETER VALUES %
%
Fluid_Parameters {
mu = 48.0             % Dynamic Viscosity (kg/(m*s)) 
rho = 1000            % Density (kg/m^3)
}


%
% TEMPORAL INFORMATION %
%
Temporal_Information {
Tfinal = 4.15e-5         % Final time of simulation (s)
dt = 1.0e-9             % Time-step (s)
}


%
% GRID PARAMETERS %
%
Grid_Parameters {
Nx =  256               % # of Eulerian Grid Pts. in x-Direction (MUST BE EVEN!!!)
Ny =  256               % # of Eulerian Grid Pts. in y-Direction (MUST BE EVEN!!!)
Lx = 400e-6                % Length of Eulerian Grid in x-Direction (m)
Ly = 400e-6                % Length of Eulerian Grid in y-Direction (m)
supp = 4                % Choose dirac-delta support (KEEP IT EVEN!!!)
}


%
% LAGRANGIAN STRUCTURE MODEL INFORMATION %
%
Lag_Structure_Info {
springs = 0             % 0 (for no) or 1 (for yes)
